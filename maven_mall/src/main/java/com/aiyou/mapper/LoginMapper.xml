<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.aiyou.mapper.LoginMapper">

	<select id="select_user_by_mch_mm" parameterType="com.aiyou.bean.T_MALL_USER_ACCOUNT"
		resultType="com.aiyou.bean.T_MALL_USER_ACCOUNT">
		select * from t_user where username = #{username}
		and
		password= #{password}
	</select>

</mapper>
